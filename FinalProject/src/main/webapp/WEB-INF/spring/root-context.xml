	<!-- Root Context: defines shared resources visible to all other web components -->

	<bean id="loginValidator" class="com.neu.edu.Validator.LoginValidator"></bean>	
	<bean id="signUpValidator" class="com.neu.edu.Validator.SignUpValidator"></bean>
	<bean id="coupleSignUpDAO" class="com.neu.edu.DAO.CoupleSignUpDAO"></bean>
